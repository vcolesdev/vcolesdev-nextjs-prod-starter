{
  "name": "vcolesdev-developer-blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "commit": "npx cz",
    "dev": "next dev",
    "lint": "eslint ./src",
    "lint:fix": "eslint . --fix",
    "makeover": "prettier --write .",
    "playwright:e2e": "npx playwright test",
    "playwright:e2e:ui": "npx playwright test --ui",
    "prepare": "husky install",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:reset-dev": "run-s prisma:wipe prisma:seed dev",
    "prisma:seed": "tsx ./prisma/functions/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push",
    "start": "next start",
    "type-check": "tsc -b",
    "vitest": "vitest --reporter=verbose"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@eslint/js": "^9.16.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "lucide-react": "^0.468.0",
    "next": "15.0.3",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "^8.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@playwright/test": "^1.49.0",
    "@prisma/client": "^6.0.1",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.7",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "happy-dom": "^15.11.7",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.0.1",
    "react-hook-form": "^7.54.0",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.8",
    "zod": "^3.23.8"
  }
}
